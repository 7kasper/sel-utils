{
	"repo": {
		"name": "D",
		"src": "src",
		"exclude": ["sul/utils/about.d"],
		"tag": true,
		"badges": [
			{
				"image": "https://img.shields.io/dub/v/sel-utils.svg",
				"url": "https://code.dlang.org/packages/sel-utils",
				"alt": "DUB Package"
			},
			{
				"image": "https://img.shields.io/dub/dt/sel-utils.svg",
				"url": "https://code.dlang.org/packages/sel-utils",
				"alt": "DUB Downloads"
			}
		]
	},
	"indentation": "spaces",
	"types": {
		"change_names": {
			"version": "vers",
			"body": "body_",
			"safe": "safe_"
		},
		"basic": {
			"bytes": "ubyte[]",
			"uuid": "UUID",
			"triad": "int",
			"varshort": "short",
			"varushort": "ushort",
			"varint": "int",
			"varuint": "uint",
			"varlong": "long",
			"varulong": "ulong"
		},
		"tuples": {
			"2": "Tuple!(%, $0, %, $1)",
			"3": "Tuple!(%, $0, %, $1, %, $2)"
		},
		"metadata": "sul.metadata.{{GAME:lowercase}}.Metadata",
		"others": "sul.protocol.{{GAME:lowercase}}.types.{{TYPE:pascal_case}}",
		"encoding": {
			"basic": "this.write{{TYPE:pascal_case}}({{NAME:camel_case}})",
			"types": "{{NAME:camel_case}}.encode()"
		}
	}
}
