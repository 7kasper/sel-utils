			"update_packet_loss": {
				"id": 17,
				"clientbound": true,
				"serverbound": false,
				"fields": [
					{
						"name": "hub_id",
						"type": "varuint"
					},
					{
						"name": "packet_loss",
						"type": "float"
					}
				]
			},

			"game_packet": {
				"id": 18,
				"clientbound": true,
				"serverbound": true,
				"fields": [
					{
						"name": "hub_id",
						"type": "varuint"
					},
					{
