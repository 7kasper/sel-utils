namespace sul\utils;

class ItemData {
	
	/** @var bool */
	public $exists;

	/** @var int */
	public $id;

	/** @var int */
	public $meta;

	function __construct($exists, int $id, int $meta) {
		$this->exists = $exists;
		$this->id = $id;
		$this->meta = $meta;
	}

}
